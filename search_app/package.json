{
  "name": "sorghumbase-meta-search",
  "version": "1.0.0",
  "description": "meta search interface for sorghumbase",
  "main": "index.js",
  "scripts": {
    "start": "parcel ./src/index.html",
    "build": "parcel build -d ../sorghum_webapp/sorghum_webapp/static/js/ -o search.js src/index.js",
    "watch": "parcel watch -d ../sorghum_webapp/sorghum_webapp/static/js/ -o search.js src/index.js",
    "build-searchbar": "parcel build -d ../sorghum_webapp/sorghum_webapp/static/js/ -o searchbar.js src/searchbar.js",
    "watch-searchbar": "parcel watch -d ../sorghum_webapp/sorghum_webapp/static/js/ -o searchbar.js src/searchbar.js"
  },
  "author": "Andrew Olson",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^4.4.1",
    "desandro-matches-selector": "^2.0.2",
    "ev-emitter": "^1.1.1",
    "fizzy-ui-utils": "^2.0.7",
    "get-size": "^2.0.3",
    "gramene-search": "^1.1.8",
    "internal-nav-helper": "1.0.2",
    "jquery": "^3.4.1",
    "money-clip": "2.0.0",
    "outlayer": "^2.1.1",
    "querystringify": "^2.0.0",
    "react": "16.12.0",
    "react-bootstrap": "^1.0.0-beta.16",
    "react-debounce-input": "^3.2.0",
    "react-dom": "^16.12.0",
    "redux-bundler": "^21.0.0",
    "redux-bundler-react": "^1.0.1",
    "sorghum-search": "^1.1.5"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "^6.24.1",
    "parcel-bundler": "^1.10.1"
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  }
}
